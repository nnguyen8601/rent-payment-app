{
  "routes": [
    {
      "route": "/api/login",
      "methods": ["POST"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/process-payment",
      "methods": ["POST"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  }
} 